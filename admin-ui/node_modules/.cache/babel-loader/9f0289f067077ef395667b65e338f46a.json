Loading = false\r\n      })\r\n    },\r\n    handleFilter() {\r\n      this.listQuery.page = 1\r\n      this.getList()\r\n    },\r\n    handleDownload() {\r\n      this.downloadLoading = true\r\n        import('@/vendor/Export2Excel').then(excel => {\r\n          const tHeader = ['商品ID', '名称', '首页主图', '折扣', '人数要求', '活动开始时间', '活动结束时间']\r\n          const filterVal = ['id', 'name', 'pic_url', 'discount', 'discountMember', 'addTime', 'expireTime']\r\n          excel.export_json_to_excel2(tHeader, this.list, filterVal, '商品信息')\r\n          this.downloadLoading = false\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";;;;;;;;;AAwFA,IAAAA,QAAA,GAAAC,OAAA;AACA,IAAAC,UAAA,GAAAC,sBAAA,CAAAF,OAAA;AACA,IAAAG,WAAA,GAAAD,sBAAA,CAAAF,OAAA;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAAA;AAAA,IAAAI,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,UAAA;IAAAC,SAAA,EAAAA,kBAAA;IAAAC,UAAA,EAAAA;EAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;MACAC,WAAA;MACAC,SAAA;QACAC,IAAA;QACAC,KAAA;QACAC,OAAA,EAAAC,SAAA;QACAC,IAAA;QACAC,KAAA;MACA;MACAC,WAAA;MACAC,mBAAA;MACAC,eAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAD,OAAA,WAAAA,QAAA;MAAA,IAAAE,KAAA;MACA,KAAAd,WAAA;MACA,IAAAe,mBAAA,OAAAd,SAAA,EAAAe,IAAA,WAAAC,QAAA;QACAH,KAAA,CAAAhB,IAAA,GAAAmB,QAAA,CAAApB,IAAA,CAAAA,IAAA,CAAAqB,KAAA;QACAJ,KAAA,CAAAf,KAAA,GAAAkB,QAAA,CAAApB,IAAA,CAAAA,IAAA,CAAAE,KAAA;QACAe,KAAA,CAAAd,WAAA;MACA,GAAAmB,KAAA;QACAL,KAAA,CAAAhB,IAAA;QACAgB,KAAA,CAAAf,KAAA;QACAe,KAAA,CAAAd,WAAA;MACA;IACA;IACAoB,YAAA,WAAAA,aAAA;MACA,KAAAnB,SAAA,CAAAC,IAAA;MACA,KAAAU,OAAA;IACA;IACAS,cAAA,WAAAA,eAAA;MAAA,IAAAC,MAAA;MACA,KAAAZ,eAAA;MACAa,OAAA,CAAAC,OAAA,GAAAR,IAAA;QAAA,WAAAS,wBAAA,CAAAjC,OAAA,EAAAN,OAAA;MAAA,GAAA8B,IAAA,WAAAU,KAAA;QACA,IAAAC,OAAA;QACA,IAAAC,SAAA;QACAF,KAAA,CAAAG,qBAAA,CAAAF,OAAA,EAAAL,MAAA,CAAAxB,IAAA,EAAA8B,SAAA;QACAN,MAAA,CAAAZ,eAAA;MACA;IACA;EACA;AACA","ignoreList":[]}]}